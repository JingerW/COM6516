import java.util.Arrays;

public class Ball {
	
	private int ends;
	private String[] paths;
	
	public Ball(int end, String[] path) {
		ends = end;
		paths = path;
	}
	
	public int getEnds() {
		return ends; 
	}
	
	public String getPath() {
		return Arrays.toString(paths);
	}
	
}
