import java.util.Arrays;

public class MultiplicationTable {
	
	public static void main(String[] args) {
	
		// create an empty new array set
		int[][] set = new int[9][9];
		
		// fill in integers in this set
		for (int i=0; i < 9; i++) {
			for (int j=0; j < 9; j++) {
				set[i][j] = (i+1)*(j+1);
			}
		}
		
//		for (int i=0; i <= 9; i++) {
//			if (i ==0) {
//				System.out.println("    ");
//			}
//		}
		
		System.out.println(Arrays.deepToString(set).replace("], ", "]\n"));
		System.out.println(Arrays.toString(set[0]).replace(",", "  "));
		
	}
}
